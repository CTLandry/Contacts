             xmlns:viewModel="clr-namespace:ContactsDemo.ViewModel"
             x:Class="ContactsDemo.Views.Page_ContactList"
             Title="Contacts">    
        <ContentPage.BindingContext>
            <viewModel:ViewModel_ContactList />
        </ContentPage.BindingContext>
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Contact List Page"
